#include<stdio.h>

void main()
{
	int arr[]={25,2,15,45,70};
	int n=5;
	int item=100;
	int i=0;
	int j=n;
	int k=3;
	for(i=0;i<n;i++)
	{
		printf(" \nindex=%d value= %d",i,arr[i]);
	}
	printf("\n");
	
	
	while(j>=k)
	{
		arr[j+1]=arr[j];
		j=j-1;
		
		
	}
	arr[k]=item;
	
	for(i=0;i<n+1;i++)
	{
		printf(" \nindex=%d value= %d",i,arr[i]);
	}
	
}
